{
    "name": "indigophp/indigo-base",
    "type": "fuel-package",
    "description": "IndigoPHP base package",
    "keywords": ["indigo", "base", "admin", "auth"],
    "homepage": "https://indigophp.com",
    "license": "MIT",
    "authors": [
        {
            "name": "M치rk S치gi-Kaz치r",
            "email": "mark.sagikazar@gmail.com"
        },
        {
            "name": "Tam치s Barta",
            "email": "barta.tamas.d@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0",
        "indigophp/fuel-core": "~1.0.0",
        "indigophp/fuel-dependency": "dev-develop",
        "indigophp/fuel-menu": "dev-develop",
        "fuelphp/fieldset": "dev-master",
        "fuelphp/validation": "dev-master",
        "league/fractal": "~0.7.0"
    },
    "autoload": {
        "psr-0": {
            "Monolog\\": "lib/",
            "Twig\\Extension\\": "lib/"
        },
        "psr-4": {
            "Indigo\\Base\\": "classes/"
        },
        "files": [
            "autoload.php"
        ]
    },
    "extra": {
        "installer-name": "base"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
